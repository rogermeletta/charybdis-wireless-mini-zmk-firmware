        RightGreaterThan {
            bindings = <&kp GREATER_THAN>;
            key-positions = <27 28>;
            layers = <3>;
        };

        BASE_or_EXTRAS {
            bindings = <&td_bore>;
            key-positions = <32 33>;
        };

